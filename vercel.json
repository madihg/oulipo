{
  "redirects": [
    {
      "source": "/singulars",
      "destination": "https://singulars.oulipo.xyz",
      "permanent": true
    },
    {
      "source": "/singulars/:path+",
      "destination": "https://singulars.oulipo.xyz/:path+",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/becoming-borders",
      "destination": "https://becoming-borders.vercel.app/becoming-borders"
    },
    {
      "source": "/becoming-borders/:path+",
      "destination": "https://becoming-borders.vercel.app/becoming-borders/:path+"
    }
  ]
}
